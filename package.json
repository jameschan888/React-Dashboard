{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react boilerplate",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/setup.js"
  },
  "scripts": {
    "start": "node index.js",
    "start-test": "NODE_ENV=ci npm run start",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "NODE_ENV=development concurrently \"npm run server\" \"npm run client\" ",
    "build": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "heroku-postbuild": "npm run build",
    "test": "TEST_NODE=test jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.0",
    "connect-redis": "^3.3.2",
    "cookie-session": "^1.3.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.1",
    "express-flash": "0.0.2",
    "express-jwt": "^5.3.0",
    "express-rate-limit": "^2.9.0",
    "express-session": "^1.15.6",
    "express-validator": "^3.2.1",
    "helmet": "^3.8.2",
    "jest": "^22.4.3",
    "jsonwebtoken": "^7.4.3",
    "moment": "^2.20.1",
    "mongoose": "^4.12.0",
    "node-persist": "^3.0.0",
    "nodemailer": "^4.4.1",
    "nodemailer-express-handlebars": "^2.0.0",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "puppeteer": "^1.3.0",
    "rate-limiter": "^0.2.0",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "request-ip": "^2.0.2",
    "request-promise": "^4.2.2",
    "useragent": "^2.2.1",
    "uuid": "^3.2.1",
    "yamlparser": "0.0.2"
  }
}
